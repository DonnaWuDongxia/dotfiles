#!/bin/bash

# Make sure we're being sourced (possibly by another script). Check for bash
# since zsh sets $0 when sourcing.
if [[ -n "$BASH_VERSION" && "${BASH_SOURCE:-$0}" == "$0" ]]; then
  echo "ERROR: $0 must be sourced."
  exit 1
fi

source "$DEV_CONF_ROOT"/scripts/chromium-env-common

export XWALK_ROOT=$PROJECT_ROOT/crosswalk

# Replace proxy setting
export http_proxy=http://proxy01.cd.intel.com:911
export https_proxy=http://proxy01.cd.intel.com:911

export TIZEN_ROOT=$HOME/tizen
export TIZEN_TEMP_BUILD_DIR=/var/tmp/xwalk-build

echo "Run 'xwalk/gyp_xwalk' in $XWALK_ROOT/src to generate projects."
export PS1="%B%F{cyan}[XWALK-TIZEN]$PS1"
